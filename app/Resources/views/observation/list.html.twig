{% if obslist|length > 0 %}
    {% for observation in obslist %}
        <div class="col s12 m6" >
            {% include 'common/cards/observation_state.html.twig' with {'observation':observation, 'action': 'detail' } only %}
        </div>
    {% endfor %}
{% else %}
    <div class="col s12" >
        {{ 'no_observation_found_in_zone'|trans }}
    </div>
{% endif %}

